<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd"
>
    <changeSet author="Homyakin" id="add-generated-contraband-id-to-raid-result">
        <addColumn tableName="personage_raid_result">
            <column name="generated_contraband_id" type="BIGINT">
                <constraints nullable="true"/>
            </column>
        </addColumn>

        <addForeignKeyConstraint
                baseColumnNames="generated_contraband_id"
                baseTableName="personage_raid_result"
                constraintName="fk-personage_raid_result-contraband_chest"
                referencedColumnNames="id"
                referencedTableName="contraband_chest"
        />
    </changeSet>
</databaseChangeLog>
